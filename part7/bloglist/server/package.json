{
  "name": "bloglist",
  "version": "1.0.0",
  "description": "bloglist application",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=developmen nodemon index.js",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --forceExit",
    "lint": "eslint .",
    "build:ui": "rmdir build /E /I && cd ../bloglist-frontend/ && npm run build && Xcopy /E /I build ..\\bloglist-backend\\build",
    "deploy": "git subtree push --prefix part5/bloglist-backend heroku main",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "start:test": "cross-env NODE_ENV=test nodemon index.js"
  },
  "author": "Aditya",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.7",
    "mongoose-unique-validator": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
